<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<category>
	    <pattern>careeradvice</pattern>
	    <template>Do you have interest in programming?
	        <button>
	            <text>Yes</text>
	            <postback>interestinprogramming</postback>
            </button>
            <button>
	            <text>No</text>
	            <postback>nointerestinprogramming</postback>
            </button>
	    </template>
    </category>
    
    <category>
	    <pattern>interestinprogramming</pattern>
	    <template>Are you good at working with data?
